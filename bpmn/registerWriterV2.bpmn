        <camunda:executionListener event="end">
          <camunda:script scriptFormat="JavaScript">var counter = execution.getVariable("counter")
var paper = execution.getVariable("papers")

for(var i=counter; i &lt; papers.length + counter; i++){
var fValue = org.camunda.bpm.engine.variable.Variables.fileValue(papers[i-counter].valueInfo.filename).file(new java.io.ByteArrayInputStream(java.util.Base64.getDecoder().decode(papers[i-counter].value))).mimeType(papers[i-counter].valueInfo.mimetype).encoding(papers[i-counter].valueInfo.encoding).create();
